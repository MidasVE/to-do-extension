{
    "env": { "es6": true, "browser": true, "commonjs": true, "node": true },
    "ecmaFeatures": {
        "arrowFunctions": true
    },
    "rules": { "react/prop-types": "off" },
    "parser": "babel-eslint",
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module",
        "ecmaFeatures": { "jsx": true, "classes": true, "defaultParams": true }
    },
    "extends": ["eslint:recommended", "plugin:react/recommended"]
}
