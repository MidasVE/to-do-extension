{
    "manifest_version": 2,
    "name": "New Tab",
    "description": "This extension overrides the new tab",
    "version": "0.0.1",
    "chrome_url_overrides": {
        "newtab": "index.html"
    },
    "browser_action": {
        "default_icon": "logo32.png"
    },
    "icons": {
        "16": "logo16.png",
        "32": "logo32.png",
        "192": "logo192.png"
    },
    "content_security_policy": "script-src 'self' 'sha256-1tld4PI1i3zMr1qgnUgNa4FNMkOqXf6kdvbrNm57YzM='; object-src 'self'",
    "permissions": []
}
